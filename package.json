{
  "name": "field-nurse-schedule",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "start": "npm run start --workspace=packages/api",
    "dev": "concurrently \"npm run dev --workspace=packages/api\" \"npm run dev --workspace=packages/ui\"",
    "build": "npm run build --workspace=packages/api",
    "test": "npm run test --workspaces"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
